{
  "Node: console.log": {
    "prefix": "clog",
    "body": "console.log(${1});",
    "description": "console.log"
  },
  "Node: console.error": {
    "prefix": "clerr",
    "body": "console.error(${1});",
    "description": "console.error"
  },
  "Node: import default": {
    "prefix": "imp",
    "body": "import ${1:module} from '${2:module}';",
    "description": "ESM default import"
  },
  "Node: import named": {
    "prefix": "imps",
    "body": "import { ${1:name} } from '${2:module}';",
    "description": "ESM named import"
  },
  "JS: arrow function": {
    "prefix": [
      "fn"
    ],
    "body": "const ${1:name} = (${2:args}) => {\n\t${3}\n};",
    "description": "Arrow function"
  },
  "JS: arrow function async": {
    "prefix": [
      "fn_async"
    ],
    "body": "const ${1:name} = async (${2:args}) => {\n\t${3}\n};",
    "description": "Async arrow function"
  },
  "JS: arrow function (implicit return)": {
    "prefix": [
      "afnr"
    ],
    "body": "const ${1:name} = (${2:args}) => ${3:expression};",
    "description": "Arrow function implicit return"
  },
  "JS: class": {
    "prefix": "class",
    "body": [
      "class ${1:Name} {",
      "\tconstructor(${2:args}) {",
      "\t\t${3}",
      "\t}",
      "",
      "\t${4:method}(${5:args}) {",
      "\t\t${6}",
      "\t}",
      "}"
    ],
    "description": "Class with constructor and method"
  },
  "JS: constructor": {
    "prefix": "ctor",
    "body": [
      "constructor(${1:args}) {",
      "\t${2}",
      "}"
    ],
    "description": "Constructor"
  },
  "JS: method": {
    "prefix": "method",
    "body": [
      "${1:name}(${2:args}) {",
      "\t${3}",
      "}"
    ],
    "description": "Class method"
  },
  "JS: for...of": {
    "prefix": "forof",
    "body": [
      "for (const ${1:item} of ${2:iterable}) {",
      "\t${3}",
      "}"
    ],
    "description": "for...of loop"
  },
  "JS: for...in": {
    "prefix": "forin",
    "body": [
      "for (const ${1:key} in ${2:object}) {",
      "\t${3}",
      "}"
    ],
    "description": "for...in loop"
  },
  "JS: for loop": {
    "prefix": "for",
    "body": [
      "for (let ${1:i} = 0; ${1:i} < ${2:count}; ${1:i}++) {",
      "\t${3}",
      "}"
    ],
    "description": "Classic for loop"
  },
  "JS: while loop": {
    "prefix": "while",
    "body": [
      "while (${1:condition}) {",
      "\t${2}",
      "}"
    ],
    "description": "While loop"
  },
  "Node: export named": {
    "prefix": "exp",
    "body": "export { ${1:name} };",
    "description": "Named export"
  },
  "Node: export default": {
    "prefix": "expd",
    "body": "export default ${1:name};",
    "description": "Default export"
  },
  "JS: map": {
    "prefix": "map",
    "body": "${1:array}.map((${2:item}) => ${3});",
    "description": "Array.map"
  },
  "JS: filter": {
    "prefix": "filter",
    "body": "${1:array}.filter((${2:item}) => ${3});",
    "description": "Array.filter"
  },
  "JS: reduce": {
    "prefix": "reduce",
    "body": "${1:array}.reduce((${2:acc}, ${3:item}) => ${4}, ${5:initial});",
    "description": "Array.reduce"
  },
  "JS: forEach": {
    "prefix": "foreach",
    "body": "${1:array}.forEach((${2:item}) => {\n\t${3}\n});",
    "description": "Array.forEach"
  },
  "JS: if": {
    "prefix": "if",
    "body": "if (${1:condition}) {\n\t${2}\n}",
    "description": "If statement"
  },
  "JS: if/else": {
    "prefix": "ife",
    "body": "if (${1:condition}) {\n\t${2}\n} else {\n\t${3}\n}",
    "description": "If/else statement"
  },
  "JS: switch": {
    "prefix": "switch",
    "body": [
      "switch (${1:expression}) {",
      "\tcase ${2:value}:",
      "\t\t${3}",
      "\t\tbreak;",
      "\tdefault:",
      "\t\t${4}",
      "}"
    ],
    "description": "Switch statement"
  },
  "JS: try/catch": {
    "prefix": "tryc",
    "body": [
      "try {",
      "\t${1}",
      "} catch (${2:err}) {",
      "\t${3:console.error(${2:err});}",
      "}"
    ],
    "description": "Try/catch"
  },
  "JS: async try/catch": {
    "prefix": "try_async",
    "body": [
      "try {",
        "\t${1}",
      "} catch (${2:err}) {",
      "\t${3:console.error(${2:err});}",
      "}"
    ],
    "description": "Async try/catch"
  },
  "JS: invariant": {
    "prefix": "inv",
    "body": [
      "if (!${1:condition}) {",
      "\tthrow new Error('${2:message}');",
      "}"
    ],
    "description": "Invariant check"
  },
  "JS: throw error": {
    "prefix": "throw",
    "body": "throw new Error('${1:message}');",
    "description": "Throw Error"
  }
}
